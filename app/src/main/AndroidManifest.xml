<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roosevelt.imagesbottomnavbar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ImagesBottomNavBarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".viewcontrollers.activities.OnBoardingActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".viewcontrollers.activities.MainActivity"
            android:label="@string/title_activity_main" />
        <activity
            android:name=".viewcontrollers.activities.ImagesActivity"
            android:label="@string/title_activity_images"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>